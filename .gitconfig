[apply]
	whitespace = nowarn
[user]
	name = Siddhartha Reddy
	email = sids@grok.in
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
	ui = auto
[merge]
	tool = diffmerge
	summary = true
[core]
	excludesfile = /Users/sids/.gitignore
	whitespace = cr-at-eol
[diff]
	tool = diffmerge
[difftool "diffmerge"]
	cmd = diffmerge \"$LOCAL\" \"$REMOTE\"
[mergetool "diffmerge"]
	cmd = "diffmerge --merge --result=\"$MERGED\" \"$LOCAL\" \"$(if test -f \"$BASE\"; then echo \"$BASE\"; else echo \"$LOCAL\"; fi)\" \"$REMOTE\""
	trustexitcode = false
